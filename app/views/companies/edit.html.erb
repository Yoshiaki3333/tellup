<p></p>
<h2会社を編集する</h2>
<%= form_for ([@list, @company]) do |f| %>
    <div></div>
    <div class="form-control">
        <%= f.text_field :name, placeholder: "#{@company.name}" %>
    </div>
    <div class="form-control">
        <%= f.text_field :adress, placeholder: "#{@company.adress}" %>
    </div>
    <div class="form-control">
        <%= f.text_field :tell, placeholder: "#{@company.tell}" %>
    </div>
    <%= f.submit "更新" %>
<% end %>
