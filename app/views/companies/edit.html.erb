<p></p>
<h2>会社登録</h2>
<%= form_for ([@list, @company]) do |f| %>
    <div></div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :name, placeholder: "#{@company.name}", :class => 'form-control'%>
     </div>
    </div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :adress, placeholder: "#{@company.adress}", :class => 'form-control' %>
     </div>
    </div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :tell, placeholder: "#{@company.tell}", :class => 'form-control' %>
      </div>
    </div>
    <%= f.submit "更新", :class => 'btn btn-primary' %>
<% end %>