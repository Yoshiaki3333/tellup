<p></p>
<h2>会社登録</h2>
<%= form_for ([@list, @company]) do |f| %>
    <div></div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :name, placeholder: "会社名", :class => 'form-control'%>
     </div>
    </div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :adress, placeholder: "住所", :class => 'form-control' %>
     </div>
    </div>
    <div class="form-group">
     <div class="col-md-4">
        <%= f.text_field :tell, placeholder: "電話番号", :class => 'form-control' %>
      </div>
    </div>
    <%= f.submit "登録", :class => 'btn btn-primary' %>
<% end %>