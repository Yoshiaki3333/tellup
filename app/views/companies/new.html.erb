<p></p>
<h2>会社登録</h2>
<%= form_for ([@list, @company]) do |f| %>
    <div></div>
    <div class="form-control">
        <%= f.text_field :name, placeholder: "会社名" %>
    </div>
    <div class="form-control">
        <%= f.text_field :adress, placeholder: "住所" %>
    </div>
    <div class="form-control">
        <%= f.text_field :tell, placeholder: "電話番号" %>
    </div>
    <%= f.submit "登録" %>
<% end %>